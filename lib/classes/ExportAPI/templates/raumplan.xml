<?xml version="1.0" encoding="ISO-8859-1"?>
<document>
    <format>
        <recommended>XLS</recommended>
        <forced>PDF</forced>
    </format>
    <permissions>
        <usertype>root</usertype>
    </permissions>
    <elements>
        <timetable>
            <time>
                <min>8</min>
                <max>22</max>
                <steps>0.5</steps>  
                <format>%H:%M</format>       
            </time>
            <date>
                <from>$0</from>
                <to>$1</to> 
                <format>%A</format>          
            </date>
            <database>
                <sql>SELECT * FROM termine 
                    LEFT JOIN seminare ON (range_id = seminar_id) 
                    LEFT JOIN seminar_user USING (seminar_id) 
                    LEFT JOIN auth_user_md5 USING (user_id)
                    LEFT JOIN resources_assign ON (range_id = assign_id)
                    WHERE raum = '$2' AND seminar_user.status = 'dozent' </sql>
                <begin>date</begin>
                <end>end_time</end>
            </database>
            <data>#VeranstaltungsNummer\n#Name\n#Nachname</data>
            <header>Raumplanäüö</header>
            <footer>blablablabal ruf da an!!!!</footer>  
        </timetable>
    </elements>
</document>